[ignore]
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/cache/.*
<PROJECT_ROOT>/coverage/.*
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/temp/.*
<PROJECT_ROOT>/var/.*

[include]

[libs]

[lints]
sketchy-null=error
sketchy-null-bool=off
untyped-type-import=error
untyped-import=error
unclear-type=error

[options]
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper.extension='jpeg' -> '<PROJECT_ROOT>/image.js'
emoji=true
esproposal.export_star_as=enable
esproposal.optional_chaining=ignore
experimental.const_params=true
module.use_strict=true
suppress_type=TODO
suppress_type=HACK

[strict]
